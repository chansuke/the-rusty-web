{
  "name": "the-rusty-web",
  "version": "0.0.1",
  "description": "Targeting the web with Rust.",
  "main": "webpack.config.js",
  "scripts": {
    "include-rust-lib": "cp ../target/asmjs-unknown-emscripten/release/the_rusty_web.js dist/",
    "build": "npm run include-rust-lib && webpack",
    "build-release": "npm run include-rust-lib && NODE_ENV=production webpack -p",
    "start": "npm run include-rust-lib && webpack-dev-server",
    "loc": "sloc src/",
    "web": "webpack -v"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "David McNeil",
  "license": "ISC",
  "dependencies": {
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-polyfill": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "css-loader": "^0.26.1",
    "eslint": "^3.15.0",
    "file-loader": "^0.10.0",
    "jquery": "^3.1.1",
    "node-sass": "^4.5.0",
    "sass-loader": "^5.0.1",
    "sloc": "^0.2.0",
    "style-loader": "^0.13.1",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.3.0"
  }
}